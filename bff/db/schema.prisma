// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model Users {
    user_id    Int      @id @default(autoincrement())
    email      String   @unique
    password   String
    type       String
    created_at DateTime @default(now())
    subject_id String?

    @@map("users")
}
